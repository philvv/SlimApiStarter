        "respect/validation": "^1.1",
        "egulias/email-validator": "^2.1",
        "php-curl-class/php-curl-class": "^8.3",
        "firebase/php-jwt": "^5.0",
        "dragonmantank/cron-expression": "^2.2",
        "illuminate/support": "^5.8",
        "illuminate/filesystem": "^5.8",
        "moontoast/math": "^1.1",
