{
    "name": "philvv/slim-api-starter",
    "type": "project",
    "description": "Slim 3 starter for JSON API with console",
    "keywords": ["Slim 3", "Slim 3 skeleton", "Slim 3 starter", "Slim 3 console", "Slim 3 cli"],
    "homepage": "https://github.com/philvv/SlimApiStarter",
    "license": "MIT",
    "authors": [
        {
            "name": "Phillip McKeown",
            "email": "phillip@viodev.com",
            "homepage": "https://www.viodev.com"
        }
